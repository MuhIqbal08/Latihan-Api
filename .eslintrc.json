{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console" : "off",
        "no-undef": "off",
        "class-methods-use-this": "off",
        "no-alert":"off"
    }
}
